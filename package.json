{
    "name": "phaserjs",
    "displayName": "Phaser JS",
    "description": "Phaser JS 2D game library code snippets.",
    "version": "0.0.1",
    "publisher": "AkshayJangir",
    "license": "SEE LICENSE IN LICENSE.md",
    "homepage": "https://github.com/axy1976/phaserJs-VScodeExtension/blob/main/README.md",
    "bugs": {
        "url": "https://github.com/axy1976/phaserJs-VScodeExtension/issues",
        "email": "akjangir1999@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/axy1976/phaserJs-VScodeExtension.git"
    },
    "icon": "asset/icon.png",
    "engines": {
        "vscode": "^1.69.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
              "id": "javascript",
              "aliases": ["javascript", "JavaScript"],
              "extensions": [".js"]
            }
        ],
        "grammars": [
            {
              "language": "javascript",
              "scopeName": "",
              "path": ""
            }
        ],
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/scene-snippets.code-snippets"
            }
        ]
    }
}
